        "import torch\n",
        "import torch.nn as nn\n",
        "import pytorch_lightning as L\n",
        "import torch.nn.functional as F\n",
        "from torch.utils.data import DataLoader, TensorDataset"
      ],
      "metadata": {
        "id": "qt3xqqjoCnG5"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "X = torch.tensor([\n",
        "    [0., 0.],\n",
        "    [0., 1.],\n",
        "    [1., 0.],\n",
        "    [1., 1.]\n",
        "])\n",
        "\n",
        "y = torch.tensor([[0.], [1.], [1.], [0.]])"
      ],
      "metadata": {
        "id": "ZdvfY02lDBkj"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "dataset = TensorDataset(X, y)\n",
        "loader = DataLoader(dataset, batch_size=4)"
      ],
      "metadata": {
        "id": "n_XwPai3DJEg"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "class XOR_MLP(L.LightningModule):\n",
        "  def __init__(self):\n",
        "    super().__init__()\n",
        "    self.model = nn.Sequential(\n",
        "        nn.Linear(2, 8),\n",
        "          nn.ReLU(),\n",
        "          nn.Linear(8, 4),\n",
        "          nn.ReLU(),\n",
        "          nn.Linear(4, 1),\n",
        "          nn.Sigmoid()\n",
        "    )\n",
        "    self.loss_fn = nn.BCELoss()\n",
        "\n",
        "\n",
        "  def forward(self, x):\n",
        "    return self.model(x)\n",
        "\n",
        "  def training_step(self, batch, batch_idx):\n",
        "    x,y = batch\n",
        "    y_hat = self(x)\n",
        "    loss = self.loss_fn(y_hat, y)\n",
        "    self.log(\"train_loss\", loss)\n",
        "    return loss\n",
        "\n",
        "  def configure_optimizers(self):\n",
        "        return torch.optim.Adam(self.parameters(), lr=0.1)"
      ],
      "metadata": {
        "id": "N1SccbUNDnrF"
      },
      "execution_count": 30,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "model = XOR_MLP()\n",
        "trainer = L.Trainer(\n",
        "    max_epochs=500,\n",
        "    enable_checkpointing=True,\n",
        "    logger=False\n",
        ")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
